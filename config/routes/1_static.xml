<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="privacy" path="/privacy">
        <default key="_controller">Symfony\Bundle\FrameworkBundle\Controller\TemplateController::templateAction</default>
        <default key="template">Static/privacy.html.twig</default>
    </route>

    <route id="impress" path="/impress">
        <default key="_controller">Symfony\Bundle\FrameworkBundle\Controller\TemplateController::templateAction</default>
        <default key="template">Static/impress.html.twig</default>
    </route>

    <route id="limits" path="/grenzwerte">
        <default key="_controller">Symfony\Bundle\FrameworkBundle\Controller\TemplateController::templateAction</default>
        <default key="template">Static/limits.html.twig</default>
    </route>

    <route id="pollutant_pm10" path="/schadstoffe/feinstaub">
        <default key="_controller">Symfony\Bundle\FrameworkBundle\Controller\TemplateController::templateAction</default>
        <default key="template">Static/Pollutant/feinstaub.html.twig</default>
    </route>

    <route id="pollutant_no2" path="/schadstoffe/stickstoffdioxid">
        <default key="_controller">Symfony\Bundle\FrameworkBundle\Controller\TemplateController::templateAction</default>
        <default key="template">Static/Pollutant/stickstoffdioxid.html.twig</default>
    </route>
</routes>
