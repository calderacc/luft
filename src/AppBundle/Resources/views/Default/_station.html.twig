<div class="card">
    <div id="map-station-{{ station.stationCode }}" class="map" class="card-img-top" style="height: 33.33vh;" data-latitude="{{ station.latitude }}" data-longitude="{{ station.longitude }}"></div>
    <div class="card-block">
        <h4 class="card-title">
            Messstation {{ station.stationCode }}
        </h4>
        <h6 class="card-sutitle text-muted">
            {{ station.title }}
        </h6>
        {% if description is defined %}
        <p class="card-text mt-2">
            {{ description }}
        </p>
        {% endif %}
    </div>
</div>
